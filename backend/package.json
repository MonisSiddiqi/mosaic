    "mime": "^4.0.6",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.0",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5",
    "stripe": "^18.1.0",
    "tmp-promise": "^3.0.3",
    "uuid": "^11.0.5"
